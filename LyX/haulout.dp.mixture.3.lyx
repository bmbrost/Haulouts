\begin_layout Standard
\noindent

\emph on
Dirichlet process concentration parameter (
\begin_inset Formula $\theta$
\end_inset

):
\emph default

\begin_inset Formula 
\begin{eqnarray*}
[\theta|\cdot] & \propto & \text{Gamma}(r+H-1,q-\sum_{h=1}^{H-1}\log(1-\eta_{h})).
\end{eqnarray*}

\end_inset

See page 553 in Gelman et al.
 (2014).
\end_layout

\begin_layout Standard
\noindent

\emph on
\begin_inset VSpace bigskip
\end_inset


\end_layout

\begin_layout Standard
\noindent

\emph on
Coefficients describing haul-out probability (
\begin_inset Formula $\boldsymbol{\beta}$
\end_inset

):
\emph default

\begin_inset Formula 
\begin{eqnarray*}
[\boldsymbol{\beta}|\cdot] & \propto & [\mathbf{u}\left(\tilde{t}\right)|\boldsymbol{\beta},\boldsymbol{\alpha}][\boldsymbol{\beta}]\\
 & \propto & {\cal N}\left(\mathbf{u}\left(\tilde{t}\right)|\mathbf{X}\left(\tilde{t}\right)\boldsymbol{\beta}+\mathbf{W}\left(\tilde{t}\right)\boldsymbol{\alpha},\mathbf{1}\right){\cal N}(\boldsymbol{\beta}\mid\mathbf{0},\sigma_{\beta}^{2}\mathbf{I})\\
 & \propto & \exp\left\{ -\frac{1}{2}\left(\mathbf{u}\left(\tilde{t}\right)-\left(\mathbf{X}\left(\tilde{t}\right)\boldsymbol{\beta}+\mathbf{W}\left(\tilde{t}\right)\boldsymbol{\alpha}\right)\right)^{\prime}\left(\mathbf{u}\left(\tilde{t}\right)-\left(\mathbf{X}\left(\tilde{t}\right)\boldsymbol{\beta}+\mathbf{W}\left(\tilde{t}\right)\boldsymbol{\alpha}\right)\right)\right\} \times\\
 &  & \qquad\exp\left\{ -\frac{1}{2}\left(\boldsymbol{\beta}-\boldsymbol{\mu}_{\beta}\right)^{\prime}\left(\sigma_{\beta}^{2}\mathbf{I}\right)^{-1}\left(\boldsymbol{\beta}-\boldsymbol{\mu}_{\beta}\right)\right\} \\
 & \propto & \exp\left\{ -\frac{1}{2}\left(\left(\mathbf{u}\left(\tilde{t}\right)-\mathbf{W}\left(\tilde{t}\right)\boldsymbol{\alpha}\right)-\mathbf{X}\left(\tilde{t}\right)\boldsymbol{\beta}\right)^{\prime}\left(\left(\mathbf{u}\left(\tilde{t}\right)-\mathbf{W}\left(\tilde{t}\right)\boldsymbol{\alpha}\right)-\mathbf{X}\left(\tilde{t}\right)\boldsymbol{\beta}\right)\right\} \times\\
 &  & \qquad\exp\left\{ -\frac{1}{2}\left(\boldsymbol{\beta}-\boldsymbol{\mu}_{\beta}\right)^{\prime}\left(\sigma_{\beta}^{2}\mathbf{I}\right)^{-1}\left(\boldsymbol{\beta}-\boldsymbol{\mu}_{\beta}\right)\right\} \\
 & \propto & \exp\left\{ -\frac{1}{2}\left(-2\left(\left(\mathbf{u}\left(\tilde{t}\right)-\mathbf{W}\left(\tilde{t}\right)\boldsymbol{\alpha}\right)\mathbf{X}\left(\tilde{t}\right)+\boldsymbol{\mu}_{\beta}\left(\sigma_{\beta}^{2}\mathbf{I}\right)^{-1}\right)\boldsymbol{\beta}+\boldsymbol{\beta}^{\prime}\left(\mathbf{X}\left(\tilde{t}\right)^{\prime}\mathbf{X}\left(\tilde{t}\right)+\left(\sigma_{\beta}^{2}\mathbf{I}\right)^{-1}\right)\boldsymbol{\beta}\right)\right\} \\
 & = & {\cal N}(\mathbf{A}^{-1}\mathbf{b},\mathbf{A}^{-1}),
\end{eqnarray*}

\end_inset

where 
\begin_inset Formula $\mathbf{A}=\mathbf{X}^{\prime}\left(\tilde{t}\right)\mathbf{X}\left(\tilde{t}\right)+\left(\sigma_{\beta}^{2}\mathbf{I}\right)^{-1}$
\end_inset

 and 
\begin_inset Formula $\mathbf{b}^{\prime}=\left(\mathbf{u}\left(\tilde{t}\right)-\mathbf{W}\left(\tilde{t}\right)\boldsymbol{\alpha}\right){}^{\prime}\mathbf{X}\left(\tilde{t}\right)+\boldsymbol{\mu}_{\beta}^{\prime}\left(\sigma_{\beta}^{2}\mathbf{I}\right)^{-1}$
\end_inset

.
\end_layout

\begin_layout Standard
\noindent

\emph on
\begin_inset VSpace bigskip
\end_inset


\end_layout

\begin_layout Standard
\noindent

\emph on
Coefficients for non-linear trend/dependence (
\begin_inset Formula $\boldsymbol{\alpha}$
\end_inset

):
\emph default

\begin_inset Formula 
\begin{eqnarray*}
[\boldsymbol{\alpha}|\cdot] & \propto & \left[\mathbf{u}\left(\tilde{t}\right)|\boldsymbol{\beta},\boldsymbol{\alpha}\right][\boldsymbol{\alpha}]\\
 & \propto & {\cal N}\left(\mathbf{u}\left(\tilde{t}\right)|\mathbf{X}\left(\tilde{t}\right)\boldsymbol{\beta}+\mathbf{W}\left(\tilde{t}\right)\boldsymbol{\alpha},\mathbf{1}\right){\cal N}(\boldsymbol{\alpha}\mid\mathbf{0},\sigma_{\alpha}^{-1}\mathbf{I})\\
 & \propto & \exp\left\{ -\frac{1}{2}\left(\mathbf{u}\left(\tilde{t}\right)-\left(\mathbf{X}\left(\tilde{t}\right)\boldsymbol{\beta}+\mathbf{W}\left(\tilde{t}\right)\boldsymbol{\alpha}\right)\right)^{\prime}\left(\mathbf{u}\left(\tilde{t}\right)-\left(\mathbf{X}\left(\tilde{t}\right)\boldsymbol{\beta}+\mathbf{W}\left(\tilde{t}\right)\boldsymbol{\alpha}\right)\right)\right\} \times\\
 &  & \qquad\exp\left\{ -\frac{1}{2}\left(\boldsymbol{\alpha}-\mathbf{0}\right)^{\prime}\left(\sigma_{\alpha}^{2}\mathbf{I}\right)^{-1}\left(\boldsymbol{\alpha}-\mathbf{0}\right)\right\} \\
 & = & {\cal N}(\mathbf{A}^{-1}\mathbf{b},\mathbf{A}^{-1}),
\end{eqnarray*}

\end_inset

where 
\begin_inset Formula $\mathbf{A}=\mathbf{W}^{\prime}\left(\tilde{t}\right)\mathbf{W}\left(\tilde{t}\right)+\left(\sigma_{\alpha}^{2}\mathbf{I}\right)^{-1}$
\end_inset

 and 
\begin_inset Formula $\mathbf{b}^{\prime}=\left(\mathbf{u}\left(\tilde{t}\right)-\mathbf{X}\left(\tilde{t}\right)\boldsymbol{\beta}\right)^{\prime}\mathbf{W}\left(\tilde{t}\right)$
\end_inset

.
\end_layout

\begin_layout Standard
\noindent

\emph on
\begin_inset VSpace bigskip
\end_inset


\end_layout

\begin_layout Standard
\noindent

\emph on
Haul-out location coefficients (
\begin_inset Formula $\boldsymbol{\gamma}$
\end_inset

):
\emph default

\begin_inset Formula 
\begin{eqnarray*}
[\boldsymbol{\gamma}|\cdot] & \propto & \prod_{h=1}^{\tilde{H}}[\boldsymbol{\mu}_{0,h}|\boldsymbol{\gamma}][\boldsymbol{\gamma}]\\
 & \propto & \prod_{h=1}^{\tilde{H}}\left(\frac{\exp\left\{ \mathbf{u}^{\prime}(\boldsymbol{\mu}_{0,h})\boldsymbol{\gamma}\right\} }{\int_{\tilde{{\cal S}}}\exp\left\{ \mathbf{u}^{\prime}(\boldsymbol{\mu}_{0})\boldsymbol{\gamma}\right\} d\boldsymbol{\mu}_{0}}\right){\cal N}(\boldsymbol{\mu}_{\gamma},\sigma_{\gamma}^{2}\mathbf{I})
\end{eqnarray*}

\end_inset

The update for 
\begin_inset Formula $\boldsymbol{\gamma}$
\end_inset

 proceeds using Metropolis-Hastings.
 Note that the product is over all occupied clusters (i.e., with non-zero
 membership) 
\begin_inset Formula $h=1,\ldots,\tilde{H}$
\end_inset

.
\end_layout

\begin_layout Standard
\noindent

\emph on
\begin_inset VSpace bigskip
\end_inset


\end_layout

\begin_layout Standard
\noindent

\emph on
Error in the observation process (
\begin_inset Formula $\sigma$
\end_inset

):
\emph default

\begin_inset Formula 
\begin{eqnarray*}
[\sigma|\cdot] & \propto & \prod_{t}[\mathbf{s}\left(t\right)|\boldsymbol{\mu}_{0,h_{t}},z\left(t\right),\sigma^{2},\sigma_{\mu}^{2}][\sigma]\\
 & \propto & \prod_{t}[\mathbf{s}\left(t\right)|\boldsymbol{\mu}_{0,h_{t}},\sigma^{2}]^{z\left(t\right)}[\mathbf{s}\left(t\right)|\boldsymbol{\mu}_{0,h_{t}},\sigma^{2},\sigma_{\mu}^{2}]^{1-z\left(t\right)}[\sigma]\\
 & \propto & \prod_{t}{\cal N}(\mathbf{s}\left(t\right)|\boldsymbol{\mu}_{0,h_{t}},\sigma^{2}\mathbf{I})^{z\left(t\right)}{\cal N}(\mathbf{s}\left(t\right)|\boldsymbol{\mu}_{0,h_{t}},\sigma^{2}\mathbf{I}+\sigma_{\mu}^{2})^{1-z\left(t\right)}
\end{eqnarray*}

\end_inset

The update for 
\begin_inset Formula $\sigma$
\end_inset

 proceeds using Metropolis-Hastings.
\end_layout

\begin_layout Standard
\noindent

\emph on
\begin_inset VSpace bigskip
\end_inset


\end_layout

\begin_layout Standard
\noindent

\emph on
Homerange dispersion parameter (
\begin_inset Formula $\sigma_{\mu}$
\end_inset

):
\emph default

\begin_inset Formula 
\begin{eqnarray*}
[\sigma_{\mu}|\cdot] & \propto & \prod_{t\}}[\mathbf{s}\left(t\right)|\boldsymbol{\mu}_{0,h_{t}},z\left(t\right),\sigma^{2},\sigma_{\mu}^{2}][\sigma_{\mu}]\\
 & \propto & \prod_{t\}}[\mathbf{s}\left(t\right)|\boldsymbol{\mu}_{0,h_{t}},\sigma^{2},\sigma_{\mu}^{2}]^{1-z\left(t\right)}[\sigma_{\mu}]\\
 & \propto & \prod_{\{t:z\left(t\right)=0\}}{\cal N}(\mathbf{s}\left(t\right)|\boldsymbol{\mu}_{0,h_{t}},\sigma^{2}\mathbf{I}+\sigma_{\mu}^{2}\mathbf{I})
\end{eqnarray*}

\end_inset

The update for 
\begin_inset Formula $\sigma_{\mu}$
\end_inset

 proceeds using Metropolis-Hastings.
 Note that the product is over all 
\begin_inset Formula $t$
\end_inset

 for which 
\begin_inset Formula $z\left(t\right)=0$
\end_inset

.
\end_layout

