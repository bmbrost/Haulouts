\begin_layout Standard
\noindent

\series bold
Fixed effects in temporal process model
\series default
 
\begin_inset Formula $\left(\boldsymbol{\beta}\right)$
\end_inset

:
\begin_inset Formula 
\begin{eqnarray*}
\left[\boldsymbol{\beta\mid}\cdot\right] & \propto & \left[\mathbf{v}\mid\boldsymbol{\beta},\boldsymbol{\alpha}\right]\left[\boldsymbol{\beta}\right]\\
 & \propto & {\cal N}\left(\mathbf{v}\mid\mathbf{X}\boldsymbol{\beta}+\mathbf{W}\boldsymbol{\alpha},\mathbf{1}\right){\cal N}(\boldsymbol{\beta}\mid\mathbf{\boldsymbol{\mu}_{\beta}},\sigma_{\beta}^{2}\mathbf{I})\\
 & \propto & \exp\left\{ -\frac{1}{2}\left(\mathbf{v}-\left(\mathbf{X}\boldsymbol{\beta}+\mathbf{W}\boldsymbol{\alpha}\right)\right)^{\prime}\left(\mathbf{v}-\left(\mathbf{X}\boldsymbol{\beta}+\mathbf{W}\boldsymbol{\alpha}\right)\right)\right\} \\
 &  & \qquad\times\exp\left\{ -\frac{1}{2}\left(\boldsymbol{\beta}-\boldsymbol{\mu}_{\beta}\right)^{\prime}\left(\sigma_{\beta}^{2}\mathbf{I}\right)^{-1}\left(\boldsymbol{\beta}-\boldsymbol{\mu}_{\beta}\right)\right\} \\
 & \propto & \exp\left\{ -\frac{1}{2}\left(\left(\mathbf{v}-\mathbf{W}\boldsymbol{\alpha}\right)-\mathbf{X}\boldsymbol{\beta}\right)^{\prime}\left(\left(\mathbf{v}-\mathbf{W}\boldsymbol{\alpha}\right)-\mathbf{X}\boldsymbol{\beta}\right)\right\} \\
 &  & \qquad\times\exp\left\{ -\frac{1}{2}\left(\boldsymbol{\beta}-\boldsymbol{\mu}_{\beta}\right)^{\prime}\left(\sigma_{\beta}^{2}\mathbf{I}\right)^{-1}\left(\boldsymbol{\beta}-\boldsymbol{\mu}_{\beta}\right)\right\} \\
 & \propto & \exp\left\{ -\frac{1}{2}\left(-2\left(\left(\mathbf{v}-\mathbf{W}\boldsymbol{\alpha}\right)^{\prime}\mathbf{X}+\boldsymbol{\mu}_{\beta}^{\prime}\left(\sigma_{\beta}^{2}\mathbf{I}\right)^{-1}\right)\boldsymbol{\beta}+\boldsymbol{\beta}^{\prime}\left(\mathbf{X}^{\prime}\mathbf{X}+\left(\sigma_{\beta}^{2}\mathbf{I}\right)^{-1}\right)\boldsymbol{\beta}\right)\right\} \\
 & = & {\cal N}(\mathbf{A}^{-1}\mathbf{b},\mathbf{A}^{-1}),
\end{eqnarray*}

\end_inset

where 
\begin_inset Formula $\mathbf{A}=\mathbf{X}{}^{\prime}\mathbf{X}+\left(\sigma_{\beta}^{2}\mathbf{I}\right)^{-1}$
\end_inset

 and 
\begin_inset Formula $\mathbf{b}^{\prime}=\left(\mathbf{v}-\mathbf{W}\boldsymbol{\alpha}\right){}^{\prime}\mathbf{X}+\boldsymbol{\mu}_{\beta}^{\prime}\left(\sigma_{\beta}^{2}\mathbf{I}\right)^{-1}$
\end_inset

.
 Note that the matrix 
\begin_inset Formula $\mathbf{X}\equiv\left\{ \mathbf{x}\left(t\right):t\in{\cal T}\right\} $
\end_inset

 contains the vectors 
\begin_inset Formula $\mathbf{x}\left(t\right)$
\end_inset

 for all times 
\begin_inset Formula $t\in{\cal T}$
\end_inset

.
 Similarly, 
\begin_inset Formula $\mathbf{W}\equiv\left\{ \mathbf{w}\left(t\right):t\in{\cal T}\right\} $
\end_inset

 is a matrix containing the vectors 
\begin_inset Formula $\mathbf{w}\left(t\right)$
\end_inset

 for all times 
\begin_inset Formula $t\in{\cal T}$
\end_inset

.
\end_layout

