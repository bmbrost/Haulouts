\begin_layout Standard
\noindent

\series bold
Stick-breaking weights
\series default
 
\begin_inset Formula $\left(\eta_{j}\right)$
\end_inset

:
\begin_inset Formula 
\begin{eqnarray*}
\left[\eta_{j}\mid\cdot\right] & \propto & \prod_{t\in{\cal T}}\left[\boldsymbol{\mu}\left(t\right)\mid\pi_{j}\right]^{1_{\left\{ \boldsymbol{\mu}\left(t\right)=\boldsymbol{\mu}_{j}\right\} }}\prod_{i=j+1}^{J}\prod_{t\in{\cal T}}\left[\boldsymbol{\mu}\left(t\right)\mid\pi_{i}\right]^{1_{\left\{ \boldsymbol{\mu}\left(t\right)=\boldsymbol{\mu}_{i}\right\} }}\left[\eta_{j}\mid1,\theta\right]\\
 & \propto & \prod_{t\in{\cal T}}\pi_{j}^{1_{\left\{ \boldsymbol{\mu}\left(t\right)=\boldsymbol{\mu}_{j}\right\} }}\prod_{i=j+1}^{J}\prod_{t\in{\cal T}}\pi_{i}^{1_{\left\{ \boldsymbol{\mu}\left(t\right)=\boldsymbol{\mu}_{i}\right\} }}\text{Beta}\left(\eta_{j}\mid1,\theta\right)\\
 & \propto & \pi_{j}^{\sum_{t\in{\cal T}}\left(1_{\left\{ \boldsymbol{\mu}\left(t\right)=\boldsymbol{\mu}_{j}\right\} }\right)}\prod_{i=j+1}^{J}\pi_{i}^{\sum_{t\in{\cal T}}\left(1_{\left\{ \boldsymbol{\mu}\left(t\right)=\boldsymbol{\mu}_{i}\right\} }\right)}\eta_{j}^{1-1}\left(1-\eta_{j}\right)^{\theta-1}\\
 & \propto & \left(\eta_{j}\prod_{l<j}\left(1-\eta_{l}\right)\right)^{m_{j}}\prod_{i=j+1}^{J}\left(\eta_{i}\prod_{l<i}\left(1-\eta_{l}\right)\right)^{m_{i}}\left(1-\eta_{j}\right)^{\theta-1}\\
 & \propto & \eta_{j}{}^{m_{j}}\prod_{i=j+1}^{J}\left(\prod_{l<i}\left(1-\eta_{l}\right)\right)^{m_{i}}\left(1-\eta_{j}\right)^{\theta-1}\\
 & \propto & \eta_{j}{}^{m_{j}}\left(1-\eta_{j}\right)^{\sum_{i=j+1}^{J}m_{i}}\left(1-\eta_{j}\right)^{\theta-1}\\
 & \propto & \eta_{j}{}^{m_{j}}\left(1-\eta_{j}\right)^{\sum_{i=j+1}^{J}m_{i}+\theta-1}\\
 & = & \text{Beta}\left(m_{j}+1,\sum_{i=j+1}^{J}m_{i}+\theta\right),
\end{eqnarray*}

\end_inset

where 
\begin_inset Formula $m_{j}=\sum_{t\in{\cal T}}\left(1_{\left\{ \boldsymbol{\mu}\left(t\right)=\boldsymbol{\mu}_{j}\right\} }\right)$
\end_inset

, i.e., the number of observed telemetry locations 
\begin_inset Formula $\left(\mathbf{s}_{c}\left(t\right)\right)$
\end_inset

 allocated to central place 
