\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\mathbf{s}_{c}\left(t\right) & \sim & \begin{cases}
{\cal N}(\boldsymbol{\mu}\left(t\right),\boldsymbol{\Sigma}_{c}), & \text{with prob. }p\left(t\right),\, y\left(t\right)=1\\
{\cal N}(\boldsymbol{\mu}\left(t\right),\boldsymbol{\widetilde{\Sigma}}_{c}), & \text{with prob. }1-p\left(t\right),\, y\left(t\right)=1\\
{\cal N}(\boldsymbol{\mu}\left(t\right),\sigma_{\mu}^{2}\mathbf{I}+\boldsymbol{\Sigma}_{c}), & \text{with prob. }p\left(t\right),\, y\left(t\right)=0\\
{\cal N}(\boldsymbol{\mu}\left(t\right),\sigma_{\mu}^{2}\mathbf{I}+\boldsymbol{\widetilde{\Sigma}}_{c}), & \text{with prob. }1-p\left(t\right),\, y\left(t\right)=0
\end{cases}\\
\boldsymbol{\Sigma}_{c} & = & \sigma_{c}^{2}\begin{bmatrix}1 & \rho_{c}\sqrt{a_{c}}\\
\rho_{c}\sqrt{a_{c}} & a_{c}
\end{bmatrix}\\
\widetilde{\boldsymbol{\Sigma}}_{c} & = & \sigma_{c}^{2}\begin{bmatrix}1 & -\rho_{c}\sqrt{a_{c}}\\
-\rho_{c}\sqrt{a_{c}} & a_{c}
\end{bmatrix}\\
\boldsymbol{\mu}\left(t\right) & \sim & \sum_{j=1}^{J}\pi_{j}\delta_{\mu_{j}}\\
\pi_{j} & = & \eta_{j}\prod_{l<j}\left(1-\eta_{l}\right)\\
\eta_{j} & \sim & \text{Beta}\left(1,\theta\right)\\
y\left(t\right) & = & \begin{cases}
0, & v\left(t\right)\le0\\
1, & v\left(t\right)>0
\end{cases}\\
v\left(t\right) & \sim & {\cal N}\left(\mathbf{x}\left(t\right)^{\prime}\boldsymbol{\beta}+\mathbf{w}\left(t\right)^{\prime}\boldsymbol{\alpha},1\right)\\
\boldsymbol{\mu}_{j} & \sim & f_{\widetilde{{\cal S}}}\left(\mathbf{S}\right)\\
\theta & \sim & \text{Gamma}\left(r_{\theta},q_{\theta}\right)\\
\boldsymbol{\beta} & \sim & {\cal N}\left(\boldsymbol{\mu}_{\beta},\sigma_{\beta}^{2}\mathbf{I}\right)\\
\boldsymbol{\alpha} & \sim & {\cal N}\left(\mathbf{0},\sigma_{\alpha}^{2}\mathbf{I}\right)\\
\log\left(\sigma_{\mu}\right) & \sim & {\cal N}\left(\mu_{\sigma},\sigma_{\sigma}^{2}\right)\\
\sigma_{\alpha}^{2} & \sim & \text{IG}\left(r_{\alpha},q_{\alpha}\right)\\
\sigma_{c} & \sim & \text{Unif}\left(l_{\sigma},u_{\sigma}\right)\\
a_{c} & \sim & \text{Unif}\left(l_{a},u_{a}\right)\\
\rho_{c} & \sim & \text{Unif}\left(l_{\rho},u_{\rho}\right)
\end{eqnarray*}

