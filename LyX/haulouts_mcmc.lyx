\begin_inset Formula 
\begin{eqnarray*}
\left[\eta_{ij}\mid\cdot\right] & \propto & \prod_{t\in{\cal T}}\left[\boldsymbol{\mu}_{i}\left(t\right)\mid\pi_{ij}\right]^{1_{\left\{ \boldsymbol{\mu}_{i}\left(t\right)=\boldsymbol{\mu}_{ij}\right\} }}\prod_{l=j+1}^{J}\prod_{t\in{\cal T}}\left[\boldsymbol{\mu}_{i}\left(t\right)\mid\pi_{il}\right]^{1_{\left\{ \boldsymbol{\mu}_{i}\left(t\right)=\boldsymbol{\mu}_{il}\right\} }}\left[\eta_{ij}\mid1,\theta_{i}\right]\\
 & \propto & \prod_{t\in{\cal T}}\pi_{ij}^{1_{\left\{ \boldsymbol{\mu}_{i}\left(t\right)=\boldsymbol{\mu}_{ij}\right\} }}\prod_{l=j+1}^{J}\prod_{t\in{\cal T}}\pi_{il}^{1_{\left\{ \boldsymbol{\mu}_{i}\left(t\right)=\boldsymbol{\mu}_{il}\right\} }}\text{Beta}\left(\eta_{ij}\mid1,\theta_{i}\right)\\
 & \propto & \pi_{ij}^{\sum_{t\in{\cal T}}\left(1_{\left\{ \boldsymbol{\mu}_{i}\left(t\right)=\boldsymbol{\mu}_{ij}\right\} }\right)}\prod_{l=j+1}^{J}\pi_{il}^{\sum_{t\in{\cal T}}\left(1_{\left\{ \boldsymbol{\mu}_{i}\left(t\right)=\boldsymbol{\mu}_{il}\right\} }\right)}\eta_{ij}^{1-1}\left(1-\eta_{ij}\right)^{\theta_{i}-1}\\
 & \propto & \left(\eta_{ij}\prod_{l<j}\left(1-\eta_{il}\right)\right)^{n_{ij}}\prod_{l=j+1}^{J}\left(\eta_{il}\prod_{m<l}\left(1-\eta_{il}\right)\right)^{n_{il}}\left(1-\eta_{ij}\right)^{\theta_{i}-1}\\
 & \propto & \eta_{ij}{}^{n_{ij}}\prod_{l=j+1}^{J}\left(\prod_{m<l}\left(1-\eta_{il}\right)\right)^{n_{il}}\left(1-\eta_{ij}\right)^{\theta_{i}-1}\\
 & \propto & \eta_{ij}{}^{n_{ij}}\left(1-\eta_{ij}\right)^{\sum_{l=j+1}^{J}n_{il}}\left(1-\eta_{ij}\right)^{\theta_{i}-1}\\
 & \propto & \eta_{ij}{}^{n_{ij}}\left(1-\eta_{ij}\right)^{\sum_{l=j+1}^{J}n_{il}+\theta_{i}-1}\\
 & = & \text{Beta}\left(n_{ij}+1,\sum_{l=j+1}^{J}n_{il}+\theta_{i}\right),
\end{eqnarray*}

\end_inset

where 
\begin_inset Formula $n_{ij}=\sum_{t\in{\cal T}}\left(1_{\left\{ \boldsymbol{\mu}_{i}\left(t\right)=\boldsymbol{\mu}_{ij}\right\} }\right)$
\end_inset

