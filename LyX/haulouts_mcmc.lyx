\begin_inset Formula 
\begin{eqnarray*}
\left[\theta_{i}\mid\cdot\right] & \propto & \prod_{j=1}^{J-1}\left[\eta_{ij}\mid1,\theta_{i}\right]\left[\theta_{i}\mid r_{\theta},q_{\theta}\right]\\
 & \propto & \prod_{j=1}^{J-1}\text{Beta}\left(\eta_{ij}\mid1,\theta_{i}\right)\text{Gamma}\left(\theta_{i}\mid r_{\theta},q_{\theta}\right)\\
 & \propto & \prod_{j=1}^{J-1}\frac{\Gamma\left(1+\theta_{i}\right)}{\Gamma\left(1\right)\Gamma\left(\theta_{i}\right)}\eta_{ij}^{1-1}\left(1-\eta_{ij}\right)^{\theta_{i}-1}\theta_{i}^{r_{\theta}-1}\exp\left\{ -q_{\theta}\theta_{i}\right\} \\
 & \propto & \left(\frac{\theta_{i}\Gamma\left(\theta_{i}\right)}{\Gamma\left(1\right)\Gamma\left(\theta_{i}\right)}\right)^{J-1}\theta_{i}^{r_{\theta}-1}\exp\left\{ -q_{\theta}\theta_{i}+\log\left(\prod_{j=1}^{J-1}\left(1-\eta_{ij}\right)^{\theta_{i}-1}\right)\right\} \\
 & \propto & \theta_{i}^{J-1+r_{\theta}-1}\exp\left\{ -q_{\theta}\theta_{i}+\sum_{j=1}^{J-1}\left(\log\left(1-\eta_{ij}\right)^{\theta_{i}}\log\left(1-\eta_{ij}\right)^{-1}\right)\right\} \\
 & \propto & \theta_{i}^{J-1+r_{\theta}-1}\exp\left\{ -q_{\theta}\theta_{i}+\sum_{j=1}^{J-1}\log\left(1-\eta_{ij}\right)^{\theta_{i}}\right\} \\
 & \propto & \theta_{i}^{J-1+r_{\theta}-1}\exp\left\{ -q_{\theta}\theta_{i}+\theta_{i}\sum_{j=1}^{J-1}\log\left(1-\eta_{ij}\right)\right\} \\
 & \propto & \theta_{i}^{J-1+r_{\theta}-1}\exp\left\{ -\theta_{i}\left(q_{\theta}-\sum_{j=1}^{J-1}\log\left(1-\eta_{ij}\right)\right)\right\} \\
 & = & \text{Gamma}\left(r_{\theta}+J-1,q_{\theta}-\sum_{j=1}^{J-1}\log\left(1-\eta_{ij}\right)\right).
\end{eqnarray*}

