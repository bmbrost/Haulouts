Model Statement
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\mathbf{s}_{ic}\left(t\right) & \sim & \begin{cases}
{\cal N}\left(\boldsymbol{\mu}_{i}\left(t\right),\boldsymbol{\Sigma}_{ic}\right), & \text{with prob. 0.5, }y_{i}\left(t\right)=1\\
{\cal N}\left(\boldsymbol{\mu}_{i}\left(t\right),\boldsymbol{\widetilde{\Sigma}}_{ic}\right), & \text{with prob. 0.5, }y_{i}\left(t\right)=1\\
{\cal N}\left(\boldsymbol{\mu}_{i}\left(t\right),\tau_{i}^{2}\mathbf{I}+\boldsymbol{\Sigma}_{ic}\right), & \text{with prob. 0.5, }y_{i}\left(t\right)=0\\
{\cal N}\left(\boldsymbol{\mu}_{i}\left(t\right),\tau_{i}^{2}\mathbf{I}+\boldsymbol{\widetilde{\Sigma}}_{ic}\right), & \text{with prob. 0.5, }y_{i}\left(t\right)=0
\end{cases}\\
\boldsymbol{\Sigma}_{ic} & = & \sigma_{ic}^{2}\begin{bmatrix}1 & \rho_{ic}\sqrt{a_{ic}}\\
\rho_{ic}\sqrt{a_{ic}} & a_{ic}
\end{bmatrix}\\
\widetilde{\boldsymbol{\Sigma}}_{ic} & = & \sigma_{ic}^{2}\begin{bmatrix}1 & -\rho_{ic}\sqrt{a_{ic}}\\
-\rho_{ic}\sqrt{a_{ic}} & a_{ic}
\end{bmatrix}\\
\boldsymbol{\mu}_{i}\left(t\right) & \sim & \sum_{j=1}^{J}\pi_{ij}\delta_{\mu_{ij}}\\
\pi_{ij} & = & \eta_{ij}\prod_{l<j}\left(1-\eta_{il}\right)\\
\eta_{ij} & \sim & \text{Beta}\left(1,\theta_{i}\right)\\
\boldsymbol{\mu}_{ij} & \sim & f_{{\cal S}}\left(\mathbf{S}_{i}\right)\\
\theta_{i} & \sim & \text{Gamma}\left(r_{\theta},q_{\theta}\right)\\
\log\left(\tau_{i}\right) & \sim & {\cal N}\left(\mu_{\tau},\sigma_{\tau}^{2}\right)\\
\sigma_{ic} & \sim & \text{Unif}\left(l_{\sigma},u_{\sigma}\right)\\
a_{ic} & \sim & \text{Unif}\left(l_{a},u_{a}\right)\\
\rho_{ic} & \sim & \text{Unif}\left(l_{\rho},u_{\rho}\right),
\end{eqnarray*}

\end_inset

where 
\begin_inset Formula $J$
\end_inset

 indicates the upper bound to the truncation approximation of the Dirichlet
 process (Sethuraman 1994, Ishwaran and James 2001), 
\begin_inset Formula $\mathbf{S}_{i}=\left\{ \mathbf{s}_{ic}\left(t\right),\forall t\right\} $
\end_inset

 is a matrix of the observed telemetry locations for individual 
\begin_inset Formula $i$
\end_inset

, and 
\begin_inset Formula $f_{{\cal S}}\left(\mathbf{S}_{i}\right)$
\end_inset

 is the kernel density estimate of 
\begin_inset Formula $\mathbf{S}_{i}$
\end_inset

 truncated and normalized over 
\begin_inset Formula ${\cal S}$
\end_inset

.
\end_layout

